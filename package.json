{
  "name": "excel-report",
  "version": "0.0.1",
  "author": "Dmitry Ovsienko",
  "description": "excel reporter",
  "main": "dist-electron/main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:electron": "babel src/electron --out-dir dist-electron --extensions \".ts,.tsx\" --source-maps",
    "build:electron:watch": "nodemon --watch src/electron --ext ts,tsx --exec \"npm run build:electron\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run build:electron && electron-forge start",
    "dev:electron": "npm run build:electron:watch & wait-on dist-electron/main.js && electron-forge start",
    "dev:full": "concurrently \"npm run dev\" \"npm run build:electron:watch\" \"npm start\"",
    "package": "npm run build:electron && electron-forge package",
    "make": "npm run build:electron && electron-forge make"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@fontsource/roboto": "5.2.8",
    "@mui/icons-material": "7.3.4",
    "@mui/material": "7.3.4",
    "dotenv": "17.2.3",
    "electron-squirrel-startup": "1.0.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-typescript": "7.27.1",
    "@electron-forge/cli": "7.8.3",
    "@electron-forge/maker-deb": "7.10.2",
    "@electron-forge/maker-rpm": "7.10.2",
    "@electron-forge/maker-squirrel": "7.10.2",
    "@electron-forge/maker-zip": "7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "7.10.2",
    "@electron-forge/plugin-fuses": "7.10.2",
    "@electron/fuses": "1.8.0",
    "@eslint/js": "9.36.0",
    "@types/electron": "1.4.38",
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "concurrently": "9.2.1",
    "electron": "38.3.0",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "nodemon": "3.1.10",
    "tsc-watch": "7.2.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.7",
    "wait-on": "9.0.1"
  }
}
